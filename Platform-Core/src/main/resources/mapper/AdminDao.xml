<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jwzhu.platform.core.user.db.AdminDao">
    <select id="getById" resultType="com.jwzhu.platform.core.user.model.Admin">
        select id, nickname, createTime, updateTime, roleCode, `status`
        from Admin
        where id = #{id}
    </select>
</mapper>